#include <stdlib.h>
#include <stdio.h>

int num1, num2, divisao;
int main()
{
    printf("Divisão de dois Números:\n");
    scanf("%d", &num1);
    printf("Digite o Segundo Número:\n");
    scanf("%d", &num2);
    if( num2 == 0){
        printf("Não se pode dividir!!!");
    } else {
        divisao = num1 / num2;
        printf("%d", divisao);
    }

    return 0;
}
